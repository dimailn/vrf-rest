{
  "name": "vrf-rest",
  "version": "0.1.5",
  "description": "REST middleware for vrf form engine",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest --no-cache",
    "build": "coffee -o lib/ -c src/",
    "pub": "npm run build && npm publish"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "humps": "^2.0.1",
    "lodash.camelcase": "^4.3.0",
    "lodash.clonedeep": "^4.5.0",
    "pluralize": "^8.0.0",
    "rails-parse-dates": "^0.1.0",
    "rails-serialize-dates": "^0.2.0",
    "url-join": "^4.0.1"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.19.0",
    "babel-cli": "^6.26.0",
    "babel-jest": "^23.2.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "coffeescript": "^2.5.1",
    "jest": "^26.6.3"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./jest.setup.js"
    ],
    "moduleFileExtensions": [
      "js",
      "coffee"
    ],
    "transform": {
      "^.+\\.coffee$": "./jest-preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/**/*.js(x)",
      "**/__tests__/**/*.test.coffee",
      "*/?(*.)(spec|test).js(x)",
      "*/?(*.)(spec|test).coffee"
    ]
  }
}
