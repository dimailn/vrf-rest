{
  "name": "vrf-rest",
  "version": "0.3.6-effects.5",
  "description": "REST effect for vrf form engine",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest --no-cache",
    "build": "npx webpack build",
    "pub": "npm run build && npm publish"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "humps": "^2.0.1",
    "lodash.camelcase": "^4.3.0",
    "lodash.clonedeep": "^4.5.0",
    "pluralize": "^8.0.0",
    "url-join": "^4.0.1"
  },
  "devDependencies": {
    "axios": "0.21.2",
    "axios-mock-adapter": "^1.19.0",
    "babel-cli": "^6.26.0",
    "babel-jest": "^23.2.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "coffee-jest": "0.0.0",
    "coffeescript": "^2.5.1",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.6",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.2",
    "vrf": "^0.50.1-effects.15",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./jest.setup.js"
    ],
    "moduleFileExtensions": [
      "js",
      "coffee",
      "ts"
    ],
    "transform": {
      "^.+\\.coffee$": "./node_modules/coffee-jest",
      "^.+\\.ts$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/**/*.js(x)",
      "**/__tests__/**/*.test.coffee",
      "*/?(*.)(spec|test).js(x)",
      "*/?(*.)(spec|test).coffee"
    ]
  }
}
